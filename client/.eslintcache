       </>\n                  ) : (\n                    <>\n                      <Link to=\"/me\">\n                        <Button\n                          onClick={handleCloseNavMenu}\n                          sx={{ my: 2, color: \"inherit\", display: \"block\" }}\n                        >\n                          Create Activity\n                        </Button>\n                      </Link>\n                      <Link to=\"/login\">\n                        <Button\n                          onClick={handleCloseNavMenu}\n                          sx={{ my: 2, color: \"inherit\", display: \"block\" }}\n                        >\n                          Log In\n                        </Button>\n                      </Link>\n                      <Link to=\"/signup\">\n                        <Button\n                          onClick={handleCloseNavMenu}\n                          sx={{ my: 2, color: \"inherit\", display: \"block\" }}\n                        >\n                          Sign Up\n                        </Button>\n                      </Link>\n                    </>\n                  )}\n                </Typography>\n              </MenuItem>\n            </Menu>\n          </Box>\n\n          {/* VolunteerMe Logo Centered when mobile */}\n\n          <Typography\n            variant=\"h5\"\n            noWrap\n            component=\"a\"\n            href=\"\"\n            sx={{\n              mr: 2,\n              display: { xs: \"flex\", md: \"none\" },\n              flexGrow: 1,\n              fontFamily: \"monospace\",\n              fontWeight: 700,\n              letterSpacing: \".3rem\",\n              color: \"inherit\",\n              textDecoration: \"none\",\n            }}\n          >\n            VolunteerMe\n          </Typography>\n\n{/* Nav Links */}\n\n          <Box sx={{ justifyContent: 'flex-end', flexGrow: 1, display: { xs: \"none\", md: \"flex\" } }}>\n            {/* If logged in show this */}\n        